{
  "name": "django_tailwind",
  "description": "",
  "scripts": {
    "build-postcss": "postcss --config . --map false --output ../static/css/styles.min.css ./src/styles.scss",
    "build-cleancss": "cleancss -o ../static/css/styles.min.css ../static/css/styles.min.css",
    "build-clean": "rimraf ../static/css && rimraf ../static/js",
    "build:css": "npm run build-clean && cross-env NODE_ENV=production npm run build-postcss && npm run build-cleancss",
    "build:js": "rimraf ../static/js && cross-env NODE_ENV=production webpack",
    "watch:js": "rimraf ../static/js && cross-env NODE_ENV=development webpack --watch",
    "build:all": "npm run build:css && npm run build:js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-cli": "^6.1.3",
    "postcss-scss": "^2.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "svelte": "^3.18.2",
    "svelte-loader": "^2.13.6",
    "tailwindcss": "^1.2.0",
    "watch": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "intersection-observer": "^0.7.0",
    "js-cookie": "^2.2.1",
    "regenerator-runtime": "^0.13.3",
    "simplebar": "^5.1.0"
  }
}

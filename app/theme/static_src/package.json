{
  "name": "noones_client",
  "description": "Client that powers the NOC Models website dashbaord",
  "scripts": {
    "build-postcss": "postcss --config . --map false --output ../static/css/styles.min.css ./src/styles.scss",
    "build-clean": "rimraf ../static/css && rimraf ../static/js",
    "build:css": "rimraf ../static/css && cross-env NODE_ENV=production npm run build-postcss",
    "build:all": "npm run build:css && npm run build",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "validate": "svelte-check"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": "> 0.25%, not dead",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.0.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tsconfig/svelte": "^1.0.0",
    "autoprefixer": "^9.7.4",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "postcss-scss": "^2.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^1.7.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "intersection-observer": "^0.7.0",
    "js-cookie": "^2.2.1",
    "regenerator-runtime": "^0.13.3",
    "simplebar": "^5.2.1",
    "svelte": "^3.49.0"
  }
}
